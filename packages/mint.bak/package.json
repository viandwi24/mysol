{
  "name": "@mysol/mint",
  "version": "1.0.0",
  "description": "package for mint mysol",
  "author": "viandwi24",
  "license": "ISC",
  "scripts": {
    "start": "node dist/js/index.js",
    "start:watch": "nodemon -e js -w dist dist/js/index.js",
    "build": "tsc",
    "build:watch": "tsc -w --preserveWatchOutput --pretty",
    "dev": "concurrently -n \"TSC,NODE\" -c \"yellow.bold, cyan.bold\" \"npm run build:watch\" \"npm run start:watch\"",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check"
  },
  "peerDependencies": {
    "typescript": "*",
    "nodemon": "*"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "ts-mocha": "^10.0.0"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.24.2"
  }
}
